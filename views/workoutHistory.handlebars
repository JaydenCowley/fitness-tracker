<h2>Your Workout History</h2>

<div id="workout-list" class="grid container">
  <div class="list-item">

    <ol class="workout-list">
      {{#each workouts as |workout|}}
      <li>
        <article class="workout">
          {{workout.activity}} on {{workout.date_}} for {{workout.duration}}
        </article>
      </li>
      {{/each}}
    </ol>

  </div>
</div>